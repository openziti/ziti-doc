{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": ["src", "docusaurus.config.ts", "plugins"],
  "paths": {
    "@openziti/*": ["./*"]
  }
}